{
  "name": "formik-semantic-ui",
  "version": "0.10.0",
  "description": "",
  "author": "justinobney",
  "license": "MIT",
  "repository": "turner-industries/formik-semantic-ui",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom --collectCoverage",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "patch": "npm version patch -m 'update version: %s'",
    "minor": "npm version minor -m 'update version: %s'"
  },
  "peerDependencies": {
    "formik": "^1.5.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-ui-react": "^0.86.0 || ^0.87.0 || ^0.88.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "formik": "^1.5.8",
    "gh-pages": "^1.2.0",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dates": "^20.3.0",
    "react-dom": "^16.13.1",
    "react-dropzone": "^5.0.1",
    "react-scripts": "^1.1.5",
    "react-testing-library": "^5.0.1",
    "react-with-direction": "^1.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-url": "^1.4.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0",
    "prop-types": "^15.7.2"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "parser": "babylon",
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5",
    "bracketSpacing": false
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "jasmine": true
    },
    "rules": {
      "semi": [
        "warn",
        "always"
      ],
      "indent": [
        "off",
        2
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "never"
      ],
      "no-use-before-define": 0
    }
  },
  "files": [
    "dist"
  ]
}
